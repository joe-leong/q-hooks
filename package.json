{
  "name": "q-hooks",
  "description": "React 业务 Hooks",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "init": "pnpm install && pnpm run build",
    "build": "pnpm -r --filter=./packages/* run build",
    "doc:dev": "dumi dev",
    "build:doc": "dumi build",
    "clean-dist": "rimraf 'packages/hooks/{lib,es,node_modules,dist}'",
    "clean": "pnpm run clean-dist && rimraf node_modules",
    "test:watch": "jest --watch",
    "test": "jest",
    "q-lint-scan": "q-lint scan",
    "q-lint-fix": "q-lint fix",
    "release": "commit-and-tag-version && git push && git push --follow-tags",
    "update:version": "tsx scripts/update-version.ts"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "author": "joe",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.0.0",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@umijs/fabric": "^4.0.1",
    "antd": "^5.8.5",
    "babel-plugin-import": "^1.13.8",
    "chalk": "^5.3.0",
    "commit-and-tag-version": "^11.2.3",
    "consola": "^3.2.3",
    "del": "^5.1.0",
    "dumi": "^2.2.7",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.6.4",
    "jest-localstorage-mock": "^2.4.26",
    "prettier": "^3.0.2",
    "q-lint": "^0.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-shadow": "^20.4.0",
    "rimraf": "^3.0.2",
    "semver": "^7.5.4",
    "ts-jest": "^29.0.5",
    "tslib": "^2.4.1",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "q-lint commit-file-scan",
      "commit-msg": "q-lint commit-msg-scan"
    }
  }
}
